<p id="notice"><%= notice %></p>

<h1>Post Marketings</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post_marketings.each do |post_marketing| %>
      <tr>
        <td><%= post_marketing.code %></td>
        <td><%= link_to 'Show', post_marketing %></td>
        <td><%= link_to 'Edit', edit_post_marketing_path(post_marketing) %></td>
        <td><%= link_to 'Destroy', post_marketing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post Marketing', new_post_marketing_path %>
